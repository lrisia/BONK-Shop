<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="215.0" prefWidth="220.0" stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.shop.MyProductController">
   <children>
      <VBox alignment="CENTER" layoutX="13.0" layoutY="14.0" prefHeight="187.0" prefWidth="195.0">
         <children>
            <Label fx:id="myProductNameLabel" alignment="CENTER" prefHeight="42.0" prefWidth="184.0" text="name">
               <font>
                  <Font name="Agency FB Bold" size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="myProductImageView" fitHeight="90.0" fitWidth="165.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/product/p990_product.png" />
               </image>
            </ImageView>
            <Label fx:id="myProductPriceLabel" alignment="CENTER" prefHeight="28.0" prefWidth="152.0" text="price">
               <font>
                  <Font name="Agency FB Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="product" />
      <String fx:value="shadow1" />
   </styleClass>
</AnchorPane>
